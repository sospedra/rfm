{
  "name": "rfm",
  "version": "0.1.1",
  "homepage": "https://rfm.sospedra.me",
  "private": false,
  "license": "SEE LICENSE IN LICENSE",
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "scripts": {
    "build:css": "postcss src/tailwind.src.css -o src/tailwind.css --env production",
    "build:js": "craco build",
    "build": "run-s build:*",
    "deploy:build": "yarn build",
    "deploy:release": "gh-pages -d build -u \"github-actions-bot <support+actions@github.com>\"",
    "deploy": "run-s deploy:*",
    "start:css": "postcss src/tailwind.src.css -o src/tailwind.css -w",
    "start:js": "craco start",
    "start": "run-p start:*",
    "test": "craco test"
  },
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/jest": "^24.0.0",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "human-number": "^1.0.5",
    "language-map": "^1.4.0",
    "markdown-to-jsx": "^6.11.4",
    "mixpanel-browser": "^2.35.0",
    "new-github-issue-url": "^0.2.1",
    "react": "^16.13.1",
    "react-content-loader": "^5.0.4",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.4.1",
    "react-spring": "^8.0.27",
    "resize-observer-polyfill": "^1.5.1",
    "swr": "^0.2.0",
    "tailwindcss": "^1.2.0",
    "typescript": "~3.7.2",
    "validator": "^13.0.0"
  },
  "devDependencies": {
    "@craco/craco": "^5.6.4",
    "@fullhuman/postcss-purgecss": "^2.1.2",
    "@types/markdown-to-jsx": "^6.11.0",
    "@types/mixpanel-browser": "^2.35.0",
    "@types/react-router-dom": "^5.1.4",
    "@types/validator": "^13.0.0",
    "gh-pages": "^2.2.0",
    "husky": "^4.2.5",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^7.1.0",
    "prettier": "^2.0.4",
    "pretty-quick": "^2.0.1",
    "purgecss": "^2.1.2"
  }
}
